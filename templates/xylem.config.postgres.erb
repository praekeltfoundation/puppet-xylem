key: <%= @postgres_secret %>
servers:
  - hostname: <%= @postgres_host %>
    username: <%= @postgres_user %>
    <%- if @postgres_password -%>
    password: <%= @postgres_password %>
    <%- end -%>
    <%- if @postgres_connect_addr -%>
    connect_addr: <%= @postgres_connect_addr %>
    <%- end -%>
